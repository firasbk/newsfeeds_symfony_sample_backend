{% block pagenewcss %}
    <link rel="stylesheet" type="text/css" href="{{ asset("/css/daterangepicker.min.css")|raw }}" >
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/css/feed_header_without_backimg.css")|raw }}" >
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/css/slick.css")|raw }}" >
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/css/slick-theme.css")|raw }}" >
    <link rel="stylesheet" type="text/css" href="{{ asset("/assets/css/feeds_common_new.css")|raw }}" >
{% endblock %}


{% block body %}
    <section class="container deals_container">
	<div class="row margin_top_30">
	    <div class="col-xs-12">
		<div class="circlimg_arrow"><i class="fas fa-angle-right"></i></div>
		<h1 class="search_title">{{'NewsFeeds Details'}}</h1>
	    </div>
	</div>

	<div class="row margin_bottom_37">
	    <div class="col-xs-12 ">
		<div class="border_top_but_row"></div>
	    </div>
	</div>
	<div class="row">
	    <div class="col-md-9 col-xs-12 left_panel">
		<div class="row no-margin">
		    <div class="col-xs-12 nopad">
			
			<div class="row no-margin">
			    <div class="col-xs-12 nopad">
		<div class="grey_img_container">
		    {% if result['newsfeeds_main_image'] is defined and result['newsfeeds_main_image'] %}
			<img class="width-100" src="{{asset(result['newsfeeds_main_image'])}}" alt="{{result['newsfeeds_main_image']}}"/>
		    {% else %}
			<img class="width-100" src="../images/default.jpg"/>
		    {% endif %}
		</div>
			    </div>
			</div>
			<div class="row no-margin">
				<div class="col-xs-12 nopad margin_top_20">
				    <p class="font-24 black">{{'Description'}}</p>
				    <p class="font-16 grey">
					{% autoescape %}
					    {{result['newsfeeds_long_description']}}
					{% endautoescape %}
				    </p>
				</div>
			    <div class="col-xs-12 nopad">
					<p class="font-24 black">{{'Date'}}</p>
					<div class="row">
						<div class="col-xs-12">
							 {{ result['newsfeeds_date']['date'] }}
						</div>
					</div>
			    </div>
                            <div class="col-sm-2 col-xs-6 col-sm-push-6 nopad xs-full_width">
                                <div class="row no-margin">
                                    <div class="xs_pad_left">
                                        <div class="col-xs-12 nopad">
                                            <a href="/" class="pink_button">{{'Back'}}</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
			</section>
			{% endblock %}
